<h1 class="subtitle position">{{name}}</h1>
<div class="section">
    <div class="country-info">
        <p><span>Capital:</span> {{capital}}</p>
        <p><span>Population:</span> {{population}}</p>
        <p><span>Languages:</span></p>
        <ul class="list">
            {{#each languages}}
            <li>{{name}}</li>
            {{/each}}
        </ul>
    </div>

    <div class="flag">
        <img src={{flag}} alt="flag">
    </div>
</div>